<body class="nav-md">
  <div class="container body">
    <div class="main_container">
      <div class="col-md-3 left_col">
        <div class="left_col scroll-view">
          <div class="navbar nav_title" style="border: 0;">
            <a href="index" class="site_title"><i class="fa fa-paw"></i> <span>FEEDER PM</span></a>
          </div>

          <div class="clearfix"></div>

          <!-- menu profile quick info -->
          <% include menu_profile %>

          <!-- /menu profile quick info -->

          <br />

          <!-- sidebar menu -->
          <% include sidebar %>
          <!-- /sidebar menu -->

          <!-- /menu footer buttons -->
          <% include sidebar-footer %>
          <!-- /menu footer buttons -->
        </div>
      </div>

      <!-- top navigation -->
      <% include top_navigation %>
      <!-- /top navigation -->

      <!-- page content -->
      <div class="right_col" role="main">
        <div class="">
          <div class="page-title">
            <div class="title_left">
              <h3>ฟอร์มเพิ่ม Feeder ใหม่</h3>
            </div>

            <div class="title_right">
              <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                <div class="input-group">
                  <input type="text" class="form-control" placeholder="Search for...">
                  <span class="input-group-btn">
                    <button class="btn btn-default" type="button">Go!</button>
                  </span>
                </div>
              </div>
            </div>
          </div>

          <div class="row">

            <div class="col-md-6 col-xs-12">
              <div class="x_panel">
                <div class="x_title">
                  <h2>กรุณากรอกข้อมูล Feeder <small>โปรดตรวจสอบข้อมูลให้ถูกต้อง ก่อนทำการบันทึก</small></h2>
                  <ul class="nav navbar-right panel_toolbox">
                    <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i
                          class="fa fa-wrench"></i></a>
                      <ul class="dropdown-menu" role="menu">
                        <li><a href="#">Settings 1</a>
                        </li>
                        <li><a href="#">Settings 2</a>
                        </li>
                      </ul>
                    </li>
                    <li><a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                  </ul>
                  <div class="clearfix"></div>
                </div>
                <div class="x_content">
                  <br />
                  <form class="form-horizontal form-label-left" id="addFeeder" method="POST" action="/insert_feeders">

                    <div class="form-group">
                      <% if (flash && flash.error) { %>
                      <p style="color: red;" id="errorMessage">
                        <%= flash.error %>
                      </p>
                      <% } %>
                      <% if (flash && flash.success) { %>
                      <p style="color: green;" id="successMessage">
                        <%= flash.success %>
                      </p>
                      <% } %>
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">Feeder ID</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <input type="text" class="form-control" name="feederId" placeholder="Default Input">
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">MFG Tooling ID</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <input type="text" class="form-control" name="mfgToolingId" placeholder="Default Input">
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">Tooling Name</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <input type="text" class="form-control" name="toolingName" placeholder="Default Input">
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">Brand</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <select class="form-control" name="brands">
                          <option value="0">---Select Model---</option>
                          <option value="ASM">ASM</option>
                          <option value="NXT">NXT</option>

                        </select>
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">Model</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <select class="form-control" name="model">
                          <option value="0">---Select Model---</option>
                          <option value="KT08C">KT08C</option>
                          <option value="KT12C">KT12C</option>
                          <option value="KT16C">KT16C</option>
                          <option value="KT24C">KT24C</option>
                          <option value="KT32C">KT32C</option>
                          <option value="KT32C">KT48C</option>
                          <option value="KT56C">KT56C</option>
                          <option value="KT44C">KT68C</option>
                          <option value="KT72C">KT72C</option>
                        </select>
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">Size</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <select class="form-control" name="size">
                          <option value="0">---Select Size---</option>
                          <option value="8 mm">8 mm</option>
                          <option value="12 mm">12 mm</option>
                          <option value="16 mm">16 mm</option>
                          <option value="24 mm">24 mm</option>
                          <option value="32 mm">32 mm</option>
                          <option value="56 mm">56 mm</option>
                          <option value="68 mm">68 mm</option>
                          <option value="72 mm">72 mm</option>
                          <option value="86 mm">86 mm</option>
                        </select>
                      </div>
                    </div>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12">Location</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <input type="text" class="form-control" name="location" placeholder="Default Input">
                      </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Status</label>
                        <div class="col-md-9 col-sm-9 col-xs-12">
                          <select class="form-control" name="status">
                            <option value="normal">---Select Status---</option>
                            <option value="normal">normal</option>
                          </select>
                        </div>
                      </div>
                    <div class="ln_solid"></div>
                    <div class="form-group">
                      <div class="col-md-9 col-sm-9 col-xs-12 col-md-offset-3">
                        <button type="button" class="btn btn-primary">Cancel</button>
                        <button type="reset" class="btn btn-primary">Reset</button>
                        <button type="submit" class="btn btn-success">Submit</button>
                      </div>
                    </div>

                  </form>
                </div>
              </div>
            </div>
            <div class="col-md-12 col-sm-12 col-xs-12">
              <div class="x_panel">
                <div class="x_title">
                  <h2 id="comeon">ข้อมูล Feeder ล่าสุด </h2>
                  <ul class="nav navbar-right panel_toolbox">
                    <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li class="dropdown">
                      <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i
                          class="fa fa-wrench"></i></a>
                      <ul class="dropdown-menu" role="menu">
                        <li><a href="#">Settings 1</a>
                        </li>
                        <li><a href="#">Settings 2</a>
                        </li>
                      </ul>
                    </li>
                    <li><a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                  </ul>
                  <div class="clearfix"></div>
                </div>
                <div class="x_content">
                  <table class="table table-hover">
                    <thead>
                      <tr>
                        <th>feeder_id</th>
                        <th>mfg_tooling_id</th>
                        <th>tooling_name</th>
                        <th>brand</th>
                        <th>model</th>
                        <th>location</th>
                        <th>create_date</th>
                        <th>status</th>
                      </tr>
                    </thead>


                    <tbody>
                      <% last10Rows.forEach((feeder) => { %>
                      <tr>
                        <td>
                          <%- feeder.feeder_id %>
                        </td>
                        <td>
                          <%- feeder.mfg_tooling_id %>
                        </td>
                        <td>
                          <%- feeder.tooling_name %></a>
                        </td>
                        <td>
                          <%- feeder.brand %></a>
                        </td>
                        <td>
                          <%- feeder.model %></a>
                        </td>
                        <td>
                          <%- feeder.location %></a>
                        </td>
                        <td>
                          <%- feeder.create_date %></a>
                        </td>
                        <td>
                          <%- feeder.status %></a>
                        </td>
                      </tr>
                      <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- /page content -->
    </div>
  </div>


</body>

<script>
  setTimeout(() => {
    $('#successMessage').fadeOut('slow');
    $('#errorMessage').fadeOut('slow');
    $('#emptyMessage').fadeOut('fast');
  }, 3000) // <-- time in milliseconds
</script>