  <script>
    async function init_morris_charts2() {
      let response = await fetch(`/feederdata`)
      let data = await response.json()
      console.log(data)
      let scrap = data.scrap[0].count
      let allFeeder = data.allFeeder[0].count
      console.log("allFeeder = " + allFeeder)
      let due = data.due[0].count
      console.log("due = " + due)
      let overDue = data.overDue[0].count
      console.log("overDue = " + overDue)
      let pDue = ((due * 100) / allFeeder).toFixed(2)
      console.log("pDue = " + pDue)
      let pOverDue = ((overDue * 100) / allFeeder).toFixed(2)
      // let pScrap = ((scrap * 100) / allFeeder).toFixed(2)

      let pNormal = (100 - pDue - pOverDue).toFixed(2)

      const s8 = data.size8Due[0].count
      const s12 = data.size12Due[0].count
      const s16 = data.size16Due[0].count
      const s24 = data.size24Due[0].count
      const s32 = data.size32Due[0].count
      const s44 = data.size44Due[0].count
      const s56 = data.size56Due[0].count
      const s68 = data.size68Due[0].count


      if (typeof (Morris) === 'undefined') { return; }
      console.log('init_morris_charts');

      if ($('#graph_donut2').length) {

        Morris.Donut({
          element: 'graph_donut2',
          data: [
            { label: 'Normal', value: pNormal },
            { label: 'Due', value: pDue },
            { label: 'Overdue', value: pOverDue }
            
          ],
          colors: ['#16C172', '#FCAB10', '#F8333C', '#A29C9B'],
          formatter: function (y) {
            return y + "%";
          },
          resize: true
        });
      }
      if ($("#graph_bar_overall").length) {
        Morris.Bar({
          element: "graph_bar_overall",
          data: [
            { device: "Size 8mm.", total:s8 },
            { device: "Size 12mm.", total: s12 },
            { device: "Size 16mm.", total: s16 },
            { device: "Size 24mm.", total: s24 },
            { device: "Size 32mm.", total: s32 },
            { device: "Size 44mm", total: s44 },
            { device: "Size 56mm.", total: s56 },
            { device: "Size 68mm", total: s68 }
          ],
          xkey: "device",
          ykeys: ["total"],
          labels: ["total"],
          barRatio: 0.4,
          barColors: ["#FCAB10", "#34495E", "#ACADAC", "#3498DB"],
          xLabelAngle: 35,
          hideHover: "auto",
          resize: true
        })
      }
    }

    $(document).ready(() => {
      init_morris_charts2()
      console.log('morris chart2 work')
    })
  </script>
